{
  "name": "ai-supervisor-agent",
  "version": "1.0.0",
  "description": "Agent AI Desktop multi-modèles avec interface unifiée",
  "main": "src/main.js",
  "scripts": {
    "start": "electron .",
    "dev": "electron . --dev",
    "build": "electron-builder",
    "build:win": "electron-builder --win",
    "build:mac": "electron-builder --mac",
    "build:linux": "electron-builder --linux",
    "mcp:chatgpt": "node mcp-servers/chatgpt.js",
    "mcp:claude": "node mcp-servers/claude.js",
    "mcp:perplexity": "node mcp-servers/perplexity.js",
    "mcp:gemini": "node mcp-servers/gemini.js",
    "mcp:all": "concurrently \"npm run mcp:chatgpt\" \"npm run mcp:claude\" \"npm run mcp:perplexity\" \"npm run mcp:gemini\"",
    "setup": "node scripts/setup.js",
    "extract-cookies": "node scripts/extract-sessions.js"
  },
  "keywords": ["ai", "electron", "n8n", "chatgpt", "claude", "perplexity", "gemini", "notion"],
  "author": "jeyiop",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.6.0",
    "electron-store": "^8.1.0",
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "ws": "^8.14.0",
    "clipboardy": "^4.0.0",
    "node-notifier": "^10.0.1",
    "systeminformation": "^5.21.0",
    "puppeteer": "^21.0.0"
  },
  "devDependencies": {
    "electron": "^27.0.0",
    "electron-builder": "^24.6.4",
    "concurrently": "^8.2.0"
  },
  "build": {
    "appId": "com.aisupervisor.agent",
    "productName": "AI Supervisor",
    "directories": {
      "output": "dist"
    },
    "files": [
      "src/**/*",
      "node_modules/**/*",
      "package.json"
    ],
    "win": {
      "target": "nsis",
      "icon": "assets/icon.ico"
    },
    "mac": {
      "target": "dmg",
      "icon": "assets/icon.icns"
    },
    "linux": {
      "target": "AppImage",
      "icon": "assets/icon.png"
    }
  }
}
